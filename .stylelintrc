{
	"plugins": ["stylelint-prettier"],
	"extends": ["stylelint-prettier/recommended"],
	"rules": {
		"prettier/prettier": true,
		"semi": false,
		"bracketSpacing": true,
		"printWidth": 140,
		"singleQuote": true,
		"tabWidth": 2,
		"useTabs": false,
		"editor.formatOnSave": true,
		"endOfLine": "auto"
	}
}
